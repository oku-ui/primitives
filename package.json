    "turbo": "^1.9.3",
    "typescript": "^5.0.4",
    "unplugin-vue-macros": "^2.1.0",
    "vite": "^4.3.3",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1",
    "vue": "^3.3.0-beta.2"
